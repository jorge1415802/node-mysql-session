<div class="container p-4">
  <div class="row">
    {{#each enlaces}}
      <div class="col-md-3">
        <div class="card text-center">
          <div class="card-body">
            <a href="{{Url}}" target="_blank">
              <h3 class="card-title text-uppercase">{{Titulo}}</h3>
            </a>
            <p class="m-2 text-center">{{Descripcion}}</p>
            <p class="m-2">Fecha: {{Fecha_Creacion}}</p>
          </div>
        </div>
      </div>
    {{/each}}
  </div>
</div>
